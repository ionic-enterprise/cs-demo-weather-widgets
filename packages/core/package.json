{
  "name": "@ionic-enterprise/cs-demo-weather-widgets",
  "version": "1.2.2",
  "description": "Weather related components used to demonstrate Stencil component library tooling",
  "module": "dist/index.js",
  "main": "dist/index.cjs.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "components/",
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@kensodemann/game-dots": "^0.0.7",
    "@stencil/core": "^2.13.0",
    "date-fns": "~2.28.0"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "~0.4.0",
    "@stencil/eslint-plugin": "~0.4.0",
    "@stencil/react-output-target": "~0.3.1",
    "@stencil/vue-output-target": "~0.6.0",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "prettier": "~2.6.2",
    "puppeteer": "~13.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-enterprise/cs-demo-weather-widgets.git"
  },
  "author": "Ionic Customer Success <support@ionic.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-enterprise/cs-demo-weather-widgets"
  },
  "homepage": "https://github.com/ionic-enterprise/cs-demo-weather-widgets",
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@ionic/prettier-config"
}
